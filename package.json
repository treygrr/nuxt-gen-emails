{
  "name": "nuxt-gen-emails",
  "version": "1.0.0",
  "description": "A powerful Nuxt 4 module for managing email templates with live preview, reactive data stores, and automatic API route generation",
  "repository": {
    "type": "git",
    "url": "https://github.com/treygrr/nuxt-gen-emails.git"
  },
  "keywords": [
    "nuxt",
    "nuxt-module",
    "email",
    "templates",
    "preview",
    "vue",
    "typescript"
  ],
  "author": "Gilbert Rogers",
  "license": "MIT",
  "type": "module",
  "bin": {
    "nuxt-gen-emails": "./dist/cli/index.mjs"
  },
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.mjs",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/types.d.mts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "npm run dev:prepare && nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
  },
  "dependencies": {
    "@nuxt/kit": "^4.2.2",
    "citty": "^0.1.6",
    "consola": "^3.2.3",
    "pathe": "^2.0.2"
  },
  "devDependencies": {
    "@nuxt/devtools": "^3.1.1",
    "@nuxt/eslint-config": "^1.12.1",
    "@nuxt/module-builder": "^1.0.2",
    "@nuxt/schema": "^4.2.2",
    "@nuxt/test-utils": "^3.22.0",
    "@types/node": "latest",
    "changelogen": "^0.6.2",
    "eslint": "^9.39.2",
    "nuxt": "^4.2.2",
    "typescript": "~5.9.3",
    "vitest": "^4.0.16",
    "vue-tsc": "^3.2.1"
  }
}
